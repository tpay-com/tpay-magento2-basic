    |(                   # $3 : Match comments
        (?:\\#|--).*?$    # - Single line comments
        |                # - Multi line (nested) comments
         /\\*             #   . comment open marker
            (?: [^/*]    #   . non comment-marker characters
                |/(?!\\*) #   . ! not a comment open
                |\\*(?!/) #   . ! not a comment close
                |(?R)    #   . recursive case
            )*           #   . repeat eventually
        \\*\\/             #   . comment close marker
    )\\s*                 # Trim after comments
